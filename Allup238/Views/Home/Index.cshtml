    <section class="slider-area slider-3">
        <div class="container-fluid custom-container">
            <div class="row">
                <div class="col-lg-8">
                    <div class="slider-active mt-30">
                        @foreach (var slider in Model.Sliders)
                        {
                            <div class="single-slider">
                                <div class="slider-image">
                                <img src="~/uploads/sliders/@slider.ImageUrl" alt="slider">
                                </div>
                                <div class="slider-content-wrapper">
                                    <div class="slider-content">
                                        <h1 data-animation="bounceInDown" data-delay="0.2s" class="main-title "><span> @slider.Title1 </span> @slider.Title1.</h1>
                                        <p data-animation="bounceInDown" data-delay="0.5s">@slider.Title2 <br> @slider.Title2</p>
                                        <a data-animation="zoomIn" data-delay="0.8s" class="main-btn " href="shop-4-column.html">Shop Now</a>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="slider-banner mt-30">
                        <a href="shop-4-column.html"><img src="~/assets/images/banner-8.jpg" alt="banner"></a>
                    </div>
                    <!-- slider banner -->
                    <div class="slider-banner mt-30">
                        <a href="shop-4-column.html"><img src="~/assets/images/banner-9.jpg" alt="banner"></a>
                    </div> 
                    <!-- slider banner -->



                </div>
            </div> <!-- row -->
        </div> <!-- container -->

    </section>

